{
    "manifest_version": 2,
    "name": "Dr. Xu",
    "version": "1.0.0",

    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "/content_scripts.js" ]
        }
    ],

    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "48": "/icon.svg"
        },
        "default_title": "Dr. Xu",
        "default_popup": "/popup.html"
    },

    "permissions": [
        "storage"
    ]
}